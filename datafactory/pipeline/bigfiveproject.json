{
	"name": "bigfiveproject",
	"properties": {
		"activities": [
			{
				"name": "Parking Bay Sensors",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET",
						"paginationRules": {
							"supportRFC5988": "true"
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['parking_zone']"
								},
								"sink": {
									"name": "parking_zone"
								}
							},
							{
								"source": {
									"path": "$['bay_id']"
								},
								"sink": {
									"name": "bay_id"
								}
							},
							{
								"source": {
									"path": "$['st_marker_id']"
								},
								"sink": {
									"name": "st_marker_id"
								}
							},
							{
								"source": {
									"path": "$['status']"
								},
								"sink": {
									"name": "status"
								}
							},
							{
								"source": {
									"path": "$['lat']"
								},
								"sink": {
									"name": "lat"
								}
							},
							{
								"source": {
									"path": "$['lon']"
								},
								"sink": {
									"name": "lon"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "MelbourneApiDataset",
						"type": "DatasetReference",
						"parameters": {
							"apiurl": "https://data.melbourne.vic.gov.au/resource/vh2v-4nfs.json"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "MelbourneADLS",
						"type": "DatasetReference",
						"parameters": {
							"filename": "ParkingBaySensors.csv"
						}
					}
				]
			}
		],
		"annotations": []
	}
}